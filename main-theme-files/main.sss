/**
 * Tab bar & tab rules
 */

#tabbarwidget * {
    background: scolor(main, normal, 255);
}

#tabbar {
    height: 42.5px;
}

QTabBar {
    background: scolor(main, normal, 255);
}

QTabBar::tab
{
    height: 42.5px;
    color: scolor(text);
}

QTabBar::tab:selected
{
    background: scolor(accent, normal);
}

QTabBar::tab:!selected
{
    background: transparent;
}

QTabBar::tab:!selected:hover
{
    background: scolor(accent, dark);
}

/* Tab bar */

#maintabbar {
    height: 42.5px;
}

#tabbarwidget * {
    height: 42.5px;
    background: scolor(main);
}

#tabbar::tab {
    height: 42.5px;
    color: scolor(text);
}

#tabbar::tab:!selected
{
    border-right: 1px solid #535355;
    height: 25px;
    margin: 5px 0px 8px 0px;
    background: transparent
}

#tabbar::tab:!selected:hover
{
    height: 35px;
    margin: 0px;
    border-right: 1px solid #535355;
    border-left: 1px solid #535355;
    background: scolor(main, dark) repeat-x;
}

#pinnedtabbar::tab {
    height: 42.5px;
    color: scolor(text);
}

#pinnedtabbar::tab:!selected
{
    border-right: 1px solid scolor(main);
    height: 30px;
    margin: 5px 0px 8px 0px;
    background: transparent
}

#pinnedtabbar::tab:!selected:hover
{
    height: 35px;
    margin: 0px;
    border-right: 1px solid #535355;
    border-left: 1px solid #535355;
    background: scolor(accent, dark) repeat-x;
}

/**
 * Add & close tab & preferences button rules
 */

#tabs-closebutton:hover
{
    border-radius: 0px;
    background-color: #C62828;
}

#tabwidget-button-addtab:hover {
    border: scolor(accent);
    background: scolor(accent);
}

#tabwidget-button-addtab {
    border-radius: 5px;
    max-height: 30px;
    max-width: 30px;
    width: 30px;
    height: 30px;
}

#tabwidget-button-mainmenu {
    border-radius: 5px;
    max-width: 30px;
    max-height: 30px;
    width: 30px;
    height: 30px;
}

#tabwidget-button-mainmenu:hover {
    background: scolor(accent);
    border: scolor(accent);
}

#tabwidget-button-mainmenu:hover {
    border: scolor(accent);
    background: scolor(accent);
}

/**
